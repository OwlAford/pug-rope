extends ../component/layout.pug

block content
  include ../component/tabbar.pug

  .main-content
    .app-btn-grp
      button.app-btn 添加公告
      button.app-btn.dangerous 批量删除

    .app-table(cellpadding='0' cellspacing='0')
      table
        tr
          th.item
            .app-checkbox-item.indeterminate
              input.app-checkbox(type='checkbox')
          th 代理昵称
          th 当日盈利
          th 当日新增
          th 当日活跃
          th 当日注单量
          th 当日投注量
          th 当日人均输赢
          th 当月盈利
          th 同比
          th 环比
        tr
          td.item
            .app-checkbox-item
              input.app-checkbox(type='checkbox')
          td GPK
          td 10,000,000
          td 10,000
          td 10,000
          td 10,000,000
          td 10,000,000,000
          td 100
          td 10,000,000
          td
            span.fc-up 10.5%
          td
            span.fc-down 10.5%
        tr.odd
          td.item
            .app-checkbox-item.checked
              input.app-checkbox(type='checkbox' checked)
          td 娱乐先锋
          td 10,000,000
          td 10,000
          td 10,000
          td 10,000,000
          td 10,000,000,000
          td 100
          td 10,000,000
          td
            span.fc-up 10.5%
          td
            span.fc-down 10.5%

  include ../component/dialog.pug 
  